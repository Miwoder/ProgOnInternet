<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <display-name>Lab24</display-name>

    <servlet>
        <servlet-name>ServletUrl</servlet-name>
        <servlet-class>ServletUrl</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletUrl</servlet-name>
        <url-pattern>/ServletUrl</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>AddLink</servlet-name>
        <servlet-class>AddLink</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddLink</servlet-name>
        <url-pattern>/AddLink</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DeleteLink</servlet-name>
        <servlet-class>DeleteLink</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DeleteLink</servlet-name>
        <url-pattern>/DeleteLink</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>AddComment</servlet-name>
        <servlet-class>AddComment</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddComment</servlet-name>
        <url-pattern>/AddComment</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>EditComment</servlet-name>
        <servlet-class>EditComment</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EditComment</servlet-name>
        <url-pattern>/EditComment</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>EditLink</servlet-name>
        <servlet-class>EditLink</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EditLink</servlet-name>
        <url-pattern>/EditLink</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Search</servlet-name>
        <servlet-class>Search</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Search</servlet-name>
        <url-pattern>/Search</url-pattern>
    </servlet-mapping>


    
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>

</web-app>

